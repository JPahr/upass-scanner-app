{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://upass.com.ua/schema/device-ios",
    "title": "Device ios schema",
    "description": "This is a schema which contains data about customer's ios os data (os version, device name, device token)",
    "type": "object",
    "properties": {
        "id": {
            "title": "Id",
            "description": "Unique device identifier (UDID)",
            "type": "string"
        },
        "version": {
            "title": "Version",
            "description": "Describes what version of ios os customer uses",
            "type": "string"
        },
        "serialNumber": {
            "title": "Serial number",
            "description": "",
            "type": "string"
        },
        "authToken": {
            "title": "Authentication token",
            "description": "",
            "type": "string"
        },
        "passId": {
            "title": "Pass type id",
            "description": "",
            "type": "string"
        },
        "pushToken": {
            "title": "Push token",
            "description": "",
            "type": "string"
        }
    }
}